<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/popup_element_first"
    android:padding="10dp"
    >
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/popupchoosedesign"
        android:padding="10dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true">

    <ImageButton
        android:background="@drawable/roundringbluebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/navigere"
        android:src="@drawable/ic_navigation_black_48dp"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:background="@drawable/roundringgreenbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mereinformation"
        android:src="@drawable/ic_info_black_48dp"
        android:layout_gravity="center_horizontal"
        android:layout_alignTop="@+id/navigere"
        android:layout_toLeftOf="@+id/navigere"
        android:layout_toStartOf="@+id/navigere"
        android:layout_marginRight="20dp" />

    <ImageButton
        android:background="@drawable/roundringyellowbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_create_black_48dp"
        android:id="@+id/tolkebilag"
        android:layout_gravity="center_horizontal"
        android:layout_alignTop="@+id/navigere"
        android:layout_toRightOf="@+id/navigere"
        android:layout_toEndOf="@+id/navigere"
        android:layout_marginLeft="20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Info"
            android:id="@+id/textView"
            android:layout_below="@+id/mereinformation"
            android:layout_alignLeft="@+id/mereinformation"
            android:layout_alignStart="@+id/mereinformation"
            android:layout_alignRight="@+id/mereinformation"
            android:layout_alignEnd="@+id/mereinformation"
            android:textColor="#000000"
            android:layout_gravity="center_horizontal"
            android:layout_alignWithParentIfMissing="false"
            android:layout_alignParentRight="false"
            android:layout_alignParentTop="false"
            android:layout_alignParentLeft="false"
            android:layout_alignParentEnd="false"
            android:layout_alignParentStart="false"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vis pÃ¥ kort"
            android:id="@+id/textView2"
            android:textColor="#000000"
            android:layout_gravity="center_horizontal"
            android:layout_alignWithParentIfMissing="false"
            android:gravity="center"
            android:layout_below="@+id/navigere"
            android:layout_alignLeft="@+id/navigere"
            android:layout_alignStart="@+id/navigere"
            android:layout_alignRight="@+id/navigere"
            android:layout_alignEnd="@+id/navigere" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Underskriv"
            android:id="@+id/textView3"
            android:textColor="#000000"
            android:layout_gravity="center_horizontal"
            android:layout_alignWithParentIfMissing="false"
            android:gravity="center"
            android:layout_below="@+id/navigere"
            android:layout_alignLeft="@+id/tolkebilag"
            android:layout_alignStart="@+id/tolkebilag"
            android:layout_alignRight="@+id/tolkebilag"
            android:layout_alignEnd="@+id/tolkebilag" />
    </RelativeLayout>
</RelativeLayout>